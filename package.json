{
  "name": "@goproperly/eslint-config-properly-base",
  "description": "Base ESLint config used by Properly",
  "license": "MIT",
  "author": "Gavin Sharp <gavin@goproperly.com>",
  "homepage": "https://github.com/GoProperly/eslint-config-properly-base#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoProperly/eslint-config-properly-base.git"
  },
  "bugs": {
    "url": "https://github.com/GoProperly/eslint-config-properly-base/issues"
  },
  "version": "1.0.14",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . || true",
    "release": "semantic-release",
    "test": "echo \"No tests yet!\" && exit 0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^7.0.18",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "semantic-release": "^15.14.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
